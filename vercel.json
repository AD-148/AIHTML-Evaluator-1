{
    "devCommand": "npm run dev",
    "buildCommand": "npm run build",
    "outputDirectory": "frontend/dist",
    "rewrites": [
        {
            "source": "/evaluate",
            "destination": "/backend/main.py"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/dist/$1"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}